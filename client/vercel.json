{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://seabite-server.vercel.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        }
      ]
    }
  ]
}